{
  "type": "module",
  "private": true,
  "scripts": {
    "clean": "rimraf src/.observablehq/cache",
    "build": "rimraf dist && observable build",
    "dev": "observable preview",
    "deploy": "observable deploy",
    "observable": "observable",
    "start": "PORT=3000 $(command -v deno || echo '/tmp/bin/deno') run -A --watch server.ts",
    "postinstall": "curl -fsSL https://deno.land/install.sh | DENO_INSTALL=/tmp sh && /tmp/bin/deno cache --allow-scripts server.ts"
  },
  "dependencies": {
    "@observablehq/framework": "^1.9.0",
    "d3-dsv": "^3.0.1",
    "d3-time-format": "^4.1.0",
    "duckdb": "^1.0.0",
    "turndown": "^7.2.0"
  },
  "devDependencies": {
    "rimraf": "^5.0.5"
  },
  "engines": {
    "node": ">=20"
  }
}
